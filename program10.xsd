		<xs:complexType>
			<xs:sequence>
				<xs:element name="sid" type="xs:int"/>
				<xs:element name="sname" type="xs:string"/>
				<xs:element name="syear" minOccurs="1" maxOccurs="5"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>